{
  "BaseUrl": "/",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Swagger": {
    "RoutePrefix": "docs",
    "RouteTemplate": "/swagger/{documentname}/swagger.json",
    "EndpointPath": "/swagger/{0}/swagger.json"
  }
}
